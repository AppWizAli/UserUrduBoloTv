<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.hiskytech.userurdubolo.Fragments.FragmentHome"
        android:label="Home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.hiskytech.userurdubolo.Fragments.FragmentSearch"
        android:label="Dashboard"
        tools:layout="@layout/fragment_search" />
    <fragment
        android:id="@+id/navigation_private"
        android:name="com.hiskytech.userurdubolo.Fragments.FragmentPrivate"
        android:label="Exclusive Content"
        tools:layout="@layout/fragment_private" />

    <fragment
        android:id="@+id/navigation_Profile"
        android:name="com.hiskytech.userurdubolo.Fragments.FragmentProfile"
        android:label="Users"
        tools:layout="@layout/fragment_profile" />
</navigation>